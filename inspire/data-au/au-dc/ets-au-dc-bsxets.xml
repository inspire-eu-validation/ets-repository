										let $LevelTwoUnitFound := $candidate/au:lowerLevelUnit[string(@xlink:href) = concat('#', string($unitFindTwo/@gml:id))]/@xlink:href
										return
											if (string($LevelTwoUnitFound) = concat('#', string($unitFindTwo/@gml:id))) then () else $unitFindTwo
									else ()
