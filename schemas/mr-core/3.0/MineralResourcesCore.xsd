								<extension base="dateTime">
									<attribute name="nilReason" type="gml:NilReasonType"/>
								</extension>
							</simpleContent>
						</complexType>
					</element>
					<element name="relatedActivity" type="mr-core:MiningActivityPropertyType" maxOccurs="unbounded">
