  <element abstract="true" name="AbstractRiskZone" substitutionGroup="gml:AbstractFeature" type="nz-core:AbstractRiskZoneType">
    <annotation>
      <documentation>-- Definition --
A risk zone is the spatial extent of a combination of the consequences of an event (hazard) and the associated probability/likelihood of its occurrence.</documentation>
    </annotation>
  </element>
  <complexType abstract="true" name="AbstractRiskZoneType">
    <complexContent>
      <extension base="gml:AbstractFeatureType">
        <sequence>
          <element name="beginLifeSpanVersion" nillable="true">
            <annotation>
