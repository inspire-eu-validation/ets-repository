  <element name="HazardArea" substitutionGroup="nz-core:AbstractHazardArea" type="nz-core:HazardAreaType">
    <annotation>
      <documentation>-- Definition --
Discrete spatial objects representing a natural hazard.</documentation>
    </annotation>
